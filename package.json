{
  "name": "node-boiler-plate",
  "version": "1.0.0",
  "description": "This is the initiate setup of node using expressjs",
  "main": "srever.js",
  "scripts": {
    "dev": "nodemon ./src/server.js",
    "test": "cross-env NODE_ENV=test jest --testTimeout=1000 --coverage",
    "lint": "npx eslint . ",
    "lint-fix": "npx eslint --fix"
  },
  "pre-commit": [
    "test"
  ],
  "jest": {
    "coverageDirectory": "./test-reoprt/",
    "coveragePathIgnorePatterns": [],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.0",
    "nodemon": "^3.1.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "pre-commit": "^1.2.2",
    "supertest": "^7.0.0"
  }
}
